{
  "name": "mod-synth",
  "version": "1.0.0",
  "description": "MOD is a new Chrome Experiment where you can create your own modular synthesizer, or emulate different synths by simply drag and drop components.",
  "author": "Lowww Ltd",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git@github.com:andrevenancio/mod-synth.io.git"
  },
  "scripts": {
    "prestart": "npm run copy-static-files",
    "start": "tsc --watch",
    "prebuild": "npm run copy-static-files",
    "build": "tsc --build",
    "copy-static-files": "copyfiles -u 2 source/_static/**/* source/_static/**/**/* source/_static/* deploy/",
    "serve": "node ./server.js",
    "heroku-postbuild": "npm run build",
    "heroku-serve": "npm run serve"
  },
  "devDependencies": {
    "@types/node": "^12.0.0",
    "@types/react": "^16.9.0",
    "@types/react-dom": "^16.9.0",
    "compression": "^1.7.4",
    "copyfiles": "^2.3.0",
    "cors": "^2.8.5",
    "express": "^4.17.1",
    "sass": "^1.26.8",
    "typescript": "~3.7.2"
  }
}
